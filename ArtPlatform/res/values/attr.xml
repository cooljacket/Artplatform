<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ChangeColorIconWithText">
        <attr name="icon" format="reference|color"></attr>
        <attr name="color" format="color"></attr>
        <attr name="text" format="string"></attr>
        <attr name="text_size" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="Titlebar">
        <attr name="titleText" format="string"></attr>
        <attr name="titleSize" format="dimension"></attr>
        <attr name="titleColor" format="color"></attr>
    </declare-styleable>
    
</resources>